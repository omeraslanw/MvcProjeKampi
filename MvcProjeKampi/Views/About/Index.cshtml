@model List<EntityLayer.Concrete.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Hakkımda Başlığı</th>
            <th>Aktif Yap</th>
            <th>Pasif Yap</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var about in Model)
        {
            <tr>
                <td>@about.AboutID</td>
                <td>Başlık</td>
                <td><a href="#" class="btn btn-warning">Aktif Yap</a></td>
                <td><a href="#" class="btn btn-danger">Pasif Yap</a></td>
            </tr>
        }
    </tbody>
</table>
@Html.Action("AboutPartial","About")